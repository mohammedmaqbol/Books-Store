<h2>Search Author</h2>
<form action="/authors" method="GET">
  <div class="input-group">
      <input type="search" class="form-control rounded"  name="name" value="<%= searchOptions.name %>" placeholder="Search" aria-label="Search" aria-describedby="search-addon" />
      <button type="submit" class="btn btn-outline-primary">search</button>
  </div>
</form>  
<% authors.forEach(author => {  %>
    <div><%= author.name %> </div>
<% }) %> 

